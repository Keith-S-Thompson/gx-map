#!/bin/sh

# $Id: cleanup-gx-map,v 1.5 2004-11-17 20:11:30-08 kst Exp $
# $Source: /home/kst/gx-map-redacted/cleanup-gx-map,v $

for file in *.in ; do
    target=`echo $file | sed 's/\.in$//'`
    if [ -f $target ] ; then
	echo "rm -f $target"
	rm -f $target
    fi
done
if [ -f .config-file ] ; then
    echo "rm -f .config-file"
    rm -f .config-file
fi
